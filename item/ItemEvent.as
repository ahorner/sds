/*ItemEvent.asCreated by: Andrew HornerContents:ItemEvent defines an Event subclass that contains information about an item being picked up or an effect completing.*/package item{		import flash.events.Event;		public class ItemEvent extends Event	{		protected var _itemObj:Item;				/**********************************************		Constructor method		**********************************************/		public function ItemEvent(eventType:String, newItemObj:Item):void		{			super(eventType);			_itemObj = newItemObj;		}				/**********************************************		The following define public methods of the 		ItemEvent class.		**********************************************/				/**********************************************		Getters and setters		**********************************************/		public function get itemObj():Item		{			return _itemObj;		}		/**********************************************		**********************************************/	}}