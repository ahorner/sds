/*Disc.asCreated by: Andrew HornerContents:Disc defines an item contaiing some information about the genre and track name of a song, as well as a reference to a Skill object, which will be unlocked for the player when they pick up the disc.*/package item{		import character.Skill;	import flash.utils.getDefinitionByName;		public class Disc extends Item	{				protected var _genre:String;		protected var _trackName:String;		protected var _skill:Skill;				/**********************************************		Constructor Method		**********************************************/		public function Disc():void		{			super();		}				/**********************************************		The following define static methods of the 		Skill class.		**********************************************/				/*		Pre:	newGenre is a String referring to a valid genre name;				newTrackName is a String referring to a valid track name.		Post:	Returns a disc of the appropriate genre, containing the requested attributes		*/		public static function makeDisc(newGenre:String, newTrackName:String):Disc		{			var newDisc:Disc = new (Class(getDefinitionByName(newGenre)))();			newDisc.genre = newGenre;			newDisc.trackName = newTrackName;			return newDisc;					}		/**********************************************		**********************************************/				/**********************************************		The following define public methods of the 		Disc class.		**********************************************/		/**********************************************		Getters and Setters		**********************************************/		public function get genre():String		{			return _genre;		}				public function set genre(newGenre:String):void		{			_genre = newGenre;		}				public function get trackName():String		{			return _trackName;		}				public function set trackName(newTrackName:String):void		{			_trackName = newTrackName;		}				public function get skill():Skill		{			return _skill;		}				public function set skill(newSkill:Skill):void		{			_skill = newSkill;		}		/**********************************************		**********************************************/	}	}