package menu{		import sound.SoundEffect;	import flash.display.MovieClip;	import flash.events.KeyboardEvent;		public class PauseMenu extends MovieClip	{				public function PauseMenu():void		{			super();		}				public function keyHandler(e:KeyboardEvent):void		{			if (e.keyCode == 13)			{				var pauseSound:SoundEffect = new SoundEffect("bellSound");				var selectEvent = new SelectionEvent(this);				this.dispatchEvent(selectEvent);			}		}	}}