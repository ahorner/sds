/*AttackEvent.asCreated by: Andrew HornerContents:AttackEvent defines an event containing information about an attack, including the attack's source, type, power, knockback, andaffected region.*/package character{		import flash.events.Event;		public class AttackEvent extends Event	{				protected var _attackType:String		protected var _power:Number;		protected var _knockback:Number;		protected var _attributeList:Array;				/**********************************************		Constructor method		**********************************************/		public function AttackEvent(newSource:Object, newType:String, newPower:Number, newKnockback:Number, newAttributeList:Array)		{			super("attackEvent", newSource);			_attackType = newType;			_knockback = newKnockback;			_power = newPower;			_attributeList = newAttributeList;		}				/**********************************************		The following define public methods of the 		AttackEvent class.		**********************************************/				/**********************************************		Getters and Setters		**********************************************/		public function get attackType():String		{			return _attackType;		}				public function get power():Number		{			return _power;		}				public function get knockback():Number		{			return _knockback;		}				public function get attributeList():Array		{			return _attributeList;		}		/**********************************************		**********************************************/	}	}